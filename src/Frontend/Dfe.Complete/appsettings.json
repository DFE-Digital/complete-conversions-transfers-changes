{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Dfe.Complete"
    }
  },
  "AllowedHosts": "*",
  "GoogleAnalytics": {
    "Enable": "No"
  },
  "Complete": {
    "ApiEndpoint": "<insert api endpoint>",
    "ApiKey": "app-key"
  },
  "Academies": {
    "ApiEndpoint": "<insert api endpoint>",
    "ApiKey": "app-key"
  },
  "CompleteApi": {
    "ApiKey": "app-key"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com",
    "Domain": "Educationgovuk.onmicrosoft.com",
    "CallbackPath": "/signin-oidc",
    "ClientId": "",
    "TenantId": "",
    "ClientSecret": "",
    "GroupId": "insert-group-id",
    "AllowedRoles": "RegionalDeliveryOfficer"
  },
  "ApplicationInsights": {
    "ConnectionString": "secret",
    "EnableBrowserAnalytics": "Yes"
  },
  "CypressTestSecret": "secret",
  "RequestHeaderKey": "x-request-origin",
  "RequestHeaderValue": "ruby",
  "AuthenticationExpirationInMinutes": 60,
  "FeatureManagement": {
  },
  "ConnectionStrings": {},
  "DataProtection": {
    "KeyVaultKey": ""
  },
  "Authorization": {
    "Policies": [
      {
        "Name": "CanViewYourProjects",
        "Operator": "Or",
        "Roles": [ "assign_to_project" ]
      },
      {
        "Name": "CanViewTeamProjects",
        "Operator": "Or",
        "Roles": [ "regional_delivery_officer", "regional_casework_services" ]
      },
      {
        "Name": "CanViewTeamProjectsHandedOver",
        "Operator": "Or",
        "Roles": [ "regional_delivery_officer" ]
      },
      {
        "Name": "CanViewGroups",
        "Operator": "Or",
        "Roles": [ "regional_delivery_officer", "regional_casework_services", "service_support" ]
      },
      {
        "Name": "ManageLocalAuthorities",
        "Operator": "And",
        "Roles": [ "service_support", "manage_local_authorities" ]
      },
      {
        "Name": "CanViewServiceSupport",
        "Operator": "Or",
        "Roles": [ "service_support" ]
      },
      {
        "Name": "CanViewAllProjectsHandover",
        "Operator": "Or",
        "Roles": [ "regional_delivery_officer", "service_support" ]
      },
      {
        "Name": "CanViewAllProjectsExports",
        "Operator": "Or",
        "Roles": [ "service_support", "business_support", "data_consumers", "manage_team" ]
      },
      {
        "Name": "CanAddNotes",
        "Operator": "Or",
        "Roles": [ "add_new_project", "assign_to_project", "manage_team" ]
      },
      {
        "Name": "HasGlobalNavigation",
        "Operator": "Or",
        "Roles": [ "regional_delivery_officer", "regional_casework_services", "service_support" ]
      },
      {
        "Name": "CanEditAddedByUser",
        "Operator": "Or",
        "Roles": [ "manage_team", "service_support" ]
      }
    ]
  },
  "AcademiesApiClient": {
    "BaseUrl": "<insert api endpoint>",
    "ApiKey": ""
  },
  "Redis": {
    "Enable": false,
    "Host": "localhost",
    "Port": 6379,
    "Password": ""
  },
  "ExternalLinks": {
    "PowerBiReports": "https://app.powerbi.com/groups/e159c176-e8f1-4839-a005-3b30064047ad/reports/3fcfae66-c913-4007-94e7-bee7433d7ff9/7edc36ec9281c86d9b87?experience=power-bi",
    "ContactDfe": "https://www.gov.uk/contact-dfe",
    "DfePersonalInformationCharter": "https://www.gov.uk/government/organisations/department-for-education/about/personal-information-charter",
    "GDPRGuide": "https://ico.org.uk/for-organisations/guide-to-data-protection/guide-to-the-general-data-protection-regulation-gdpr/",
    "InformationCommissionerOffice": "https://ico.org.uk/make-a-complaint/",
    "DeedOfVariationLink": "https://educationgovuk.sharepoint.com/sites/lvewp00030/SitePages/Funding-Agreement.aspx",
    "ContactPolicyTeam": "https://educationgovuk.sharepoint.com/sites/lvewp00030/SitePages/Resources/Request%20for%20advice%20from%20the%20AOPU.aspx"
  }
}

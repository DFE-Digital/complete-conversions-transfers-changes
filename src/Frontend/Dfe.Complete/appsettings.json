{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Dfe.Complete"
    }
  },
  "AllowedHosts": "*",
  "GoogleAnalytics": {
    "Enable": "No"
  },
  "Complete": {
    "ApiEndpoint": "<insert api endpoint>",
    "ApiKey": "app-key"
  },
  "Academies": {
    "ApiEndpoint": "<insert api endpoint>",
    "ApiKey": "app-key"
  },
  "CompleteApi": {
    "ApiKey": "app-key"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com",
    "Domain": "Educationgovuk.onmicrosoft.com",
    "CallbackPath": "/signin-oidc",
    "ClientId": "",
    "TenantId": "",
    "ClientSecret": "",
    "GroupId": "insert-group-id",
    "AllowedRoles": "RegionalDeliveryOfficer"
  },
  "ApplicationInsights": {
    "ConnectionString": "secret"
  },
  "CypressTestSecret": "secret",
  "AuthenticationExpirationInMinutes": 60,
  "FeatureManagement": {
  },
  "ConnectionStrings": {
    "BlobStorage": ""
  },
  "Authorization": {
    "Policies": [
      {
        "Name": "CanCreateProjects",
        "Operator": "Or",
        "Roles": [ "RegionalDeliveryOfficer", "regional_delivery_officer" ]
      },
      {
        "Name": "CanManageUserAccount",
        "Operator": "Or",
        "Roles": [ "manage_user_accounts" ]
      }
    ]
  },
  "AcademiesApiClient": {
    "BaseUrl": "<insert api endpoint>",
    "ApiKey": ""
  }
}

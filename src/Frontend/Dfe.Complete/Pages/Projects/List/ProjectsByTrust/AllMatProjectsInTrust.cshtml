@using Dfe.Complete.Pages.Projects.List.AllProjectsInTrust
@model Dfe.Complete.Pages.Projects.List.AllProjectsInTrust.AllProjectsInTrustViewModel
@{
    Layout = "List/ProjectsByTrust/_ProjectsByTrustLayout";
    ViewData["Title"] = $"Projects for {Model.Trust.TrustName}";
}
@section PageHeading
{
    <h1 class="govuk-heading-l">
       Projects for @Model.Trust.TrustName
    </h1>
}
<partial name="_ProjectsTable" model=@Model.Trust.Projects/>
@page "/projects/{projectId}/tasks/share_information"
@model Dfe.Complete.Pages.Projects.TaskList.Tasks.ShareInformationAboutOpening.ShareInformationAboutOpeningModel
@{
    Layout = "_ProjectTaskLayout";
    ViewData["Title"] = "Share the information about opening";
}
@section TaskHeaderHintSection {
    <div class="govuk-hint"> 
    </div>
}
@section TaskSection {

    <form method="post"> 

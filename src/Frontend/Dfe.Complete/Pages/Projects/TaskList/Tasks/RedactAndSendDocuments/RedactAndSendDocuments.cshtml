@using Dfe.Complete.Domain.Enums 
@model Dfe.Complete.Pages.Projects.TaskList.Tasks.RedactAndSendDocuments.RedactAndSendDocumentsModel


@if (Model.Project.Type == ProjectType.Conversion)
{
    @await Html.PartialAsync("_ConversionRedactAndSendDocuments", Model)
}
else
{ 
    @await Html.PartialAsync("_TransferRedactAndSendDocuments", Model)
} 